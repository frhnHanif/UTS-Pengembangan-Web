<%- include('partials/_header'); -%>

<!-- Navigation Bar -->
<nav class="navbar navbar-dark bg-dark">
    <a href="/" class="navbar-brand">
        <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQL-eihGmdR1bG0MjePi9uWmQSfSnkkIligAA&s" width="30" height="30" class="d-inline-block align-top" alt="Logo">
        Database Teknik Elektro
    </a>
</nav>

<div class="container mt-5">
    <!-- Form Update Data -->
    <div class="row">
        <div class="col-md-5">
            <div class="card">
                <div class="card-body">
                    <form action="/update/<%= data.id %>" method="post">
                        <div class="form-group">
                            <input type="text" name="name" value="<%= data.name %>" class="form-control" >
                        </div>
                        <div class="form-group">
                            <input type="text" name="address" value="<%= data.address %>" class="form-control" >
                        </div>
                        <div class="form-group">
                            <input type="text" name="angkatan" value="<%= data.angkatan %>" class="form-control" >
                        </div>
                        <div class="form-group">
                            <input type="text" name="phone" value="<%= data.phone %>" class="form-control" >
                        </div>
                        <button type="submit" class="btn btn-primary">
                            Update Data
                        </button>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>

<%- include('partials/_footer'); -%>